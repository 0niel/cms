{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "Address",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "map_link": {
      "type": "string",
      "required": true,
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    }
  }
}
